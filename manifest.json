{
  "name": "WhatsApp-Web-Plus",
  "version": "1.1.6",
  "description": "",
  "permissions": [
    "storage",
    "https://web.whatsapp.com/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://web.whatsapp.com/*"
      ],
      "js": [
        "./index.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2,
  "web_accessible_resources": [
    "packed.js"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "whatsapp-web-plus@extension"
    }
  }
}